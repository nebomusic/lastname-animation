// setup function

void setup() {
  size(600, 400); // Sets the canvas size
  background(0, 249, 255); // Set background color
  
  fill(84, 255, 0); // Green fill
  strokeWeight(5);
  stroke(0, 0, 255); // blue
  rect(300, 200, 100, 100); // draw a square
  
  // triangle
  fill (255, 0, 226);
  triangle(300, 200, 350, 125, 400, 200);
  
  
}  // end setup

